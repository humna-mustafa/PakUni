{
  "name": "PakUni",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release": "cd android && gradlew assembleRelease",
    "android:bundle": "cd android && gradlew bundleRelease",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean": "cd android && gradlew clean && cd ..",
    "import-data": "npx ts-node data-import/scripts/import.ts",
    "export-data": "npx ts-node data-import/scripts/export.ts",
    "validate-data": "npx ts-node data-import/scripts/validate.ts",
    "view-import-logs": "node -e \"const fs = require('fs'); console.log(fs.readFileSync('data-import/logs/history.json', 'utf8'))\"",
    "scan:secrets": "node scripts/scan-secrets.js",
    "scan:secrets:staged": "node scripts/scan-secrets.js --staged",
    "scan:secrets:history": "node scripts/scan-secrets.js --history",
    "security:check": "npm run scan:secrets"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native/new-app-screen": "0.83.1",
    "@react-navigation/bottom-tabs": "^7.9.1",
    "@react-navigation/native": "^7.1.27",
    "@react-navigation/native-stack": "^7.9.1",
    "@supabase/supabase-js": "^2.90.1",
    "@types/react-native-vector-icons": "^6.4.18",
    "react": "19.2.0",
    "react-native": "0.83.1",
    "react-native-config": "^1.6.1",
    "react-native-device-info": "^15.0.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.19.0",
    "react-native-share": "^12.2.2",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-view-shot": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "^20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.83.1",
    "@react-native/eslint-config": "0.83.1",
    "@react-native/metro-config": "0.83.1",
    "@react-native/typescript-config": "0.83.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.13",
    "@types/node": "^25.0.8",
    "@types/react": "^19.2.0",
    "@types/react-test-renderer": "^19.1.0",
    "csv-parse": "^5.5.5",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.2.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
